let html = '';

const statusColors = {
'Completed': 'bg-emerald-100 text-emerald-800 border-emerald-200',
'In Progress': 'bg-blue-100 text-blue-800 border-blue-200',
'Pending': 'bg-gray-100 text-gray-600 border-gray-200',
'Scheduled': 'bg-purple-100 text-purple-800 border-purple-200',
'Recurring': 'bg-indigo-100 text-indigo-800 border-indigo-200'
};

deliverablesGallery.forEach(del => {
const statusClass = statusColors[del.status] || 'bg-gray-100 text-gray-800';

html += `
<div class="del-card flex flex-col h-full bg-white border border-gray-200 rounded-lg p-5 hover:shadow-lg transition-shadow duration-300">
<div class="flex justify-between items-start mb-2">
<span class="text-xs font-bold tracking-wider text-gray-400 uppercase">${del.id}</span>
<span class="px-2 py-1 rounded text-[10px] font-bold uppercase tracking-wide border ${statusClass}">
${del.status}
</span>
</div>

<div class="mb-4 flex-grow">
<span class="text-xs font-semibold tracking-wide text-blue-600 uppercase block mb-1">${del.phase}</span>
<h4 class="text-lg font-bold text-gray-900 leading-tight">${del.title}</h4>
<span class="text-xs text-gray-500 mt-1 block">${del.category}</span>
</div>

<div class="pt-4 border-t border-gray-100 text-sm space-y-1.5">
<div class="flex justify-between">
<span class="text-gray-500 text-xs">Producer</span>
<span class="font-semibold text-gray-900 text-right text-xs truncate max-w-[120px]" title="${del.producer}">${del.producer}</span>
</div>
<div class="flex justify-between">
<span class="text-gray-500 text-xs">Approver</span>
<span class="font-semibold text-gray-900 text-right text-xs truncate max-w-[120px]" title="${del.approver}">${del.approver}</span>
</div>
</div>
</div>
`;
});

container.innerHTML = html;
}

// Initialize Phase Carousel, Matrix & Deliverables
updatePhase();
renderMatrix();
renderDeliverables();

// Mobile menu toggle
const btn = document.querySelector('button.mobile-menu-btn');
const menu = document.getElementById('mobile-menu');

btn.addEventListener('click', () => {
menu.classList.toggle('hidden');
});

</script>
</body>
</html>