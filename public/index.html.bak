let currentPhaseIndex = 0;
const phaseDisplay = document.getElementById('phaseDisplay');
const phaseIndicators = document.getElementById('phaseIndicators');

function updatePhase() {
// Fade out
phaseDisplay.classList.add('fade-out');
phaseDisplay.classList.remove('fade-in');

setTimeout(() => {
phaseDisplay.innerHTML = `
${phases[currentPhaseIndex].icon}
<h2 class="text-3xl font-bold mb-4 text-black">${phases[currentPhaseIndex].title}</h2>
<p class="text-lg text-gray-700 max-w-lg">${phases[currentPhaseIndex].desc}</p>
`;
// Fade in
phaseDisplay.classList.remove('fade-out');
phaseDisplay.classList.add('fade-in');
}, 300);

// Update indicators
phaseIndicators.innerHTML = phases.map((_, idx) => `
<div class="h-2 w-2 rounded-full transition-colors duration-300 ${idx === currentPhaseIndex ? 'bg-black' : 'bg-gray-300'}"></div>
`).join('');
}

function nextPhase() {
currentPhaseIndex = (currentPhaseIndex + 1) % phases.length;
updatePhase();
}

function prevPhase() {
currentPhaseIndex = (currentPhaseIndex - 1 + phases.length) % phases.length;
updatePhase();
}

