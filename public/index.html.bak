// Initial State Check
function updateToggleState() {
if(gcToggle.checked) {
// Yes State
optYes.classList.add('text-black', 'font-bold');
optYes.classList.remove('text-gray-500');
optNo.classList.remove('text-black', 'font-bold');
optNo.classList.add('text-gray-500');

workflowContent.innerHTML = `
<div class="animate-fade-in">
<h3 class="text-xl font-bold mb-3 text-black">Scenario: Envision Construction performs as GC/HC</h3>
<p class="text-gray-700 font-medium mb-3">Direct field operations, trade management, safety, schedule, and quality control.</p>
<p class="text-gray-600 text-sm leading-relaxed">In this scenario, Envision Construction takes on the risk and responsibility of the General Contractor. They directly hire subcontractors and manage day-to-day site logistics.</p>
</div>
`;
} else {
// No State
optNo.classList.add('text-black', 'font-bold');
optNo.classList.remove('text-gray-500');
optYes.classList.remove('text-black', 'font-bold');
optYes.classList.add('text-gray-500');

workflowContent.innerHTML = `
<div class="animate-fade-in">
<h3 class="text-xl font-bold mb-3 text-black">Scenario: Envision Construction manages third-party GC</h3>
<p class="text-gray-700 leading-relaxed">Envision Construction acts purely as the Development Manager. They oversee the hiring and performance of an external General Contractor. Focus is on high-level schedule, budget adherence, and quality assurance.</p>
</div>
`;
}
}

gcToggle.addEventListener('change', updateToggleState);

// Allow clicking labels to toggle
optNo.addEventListener('click', () => { gcToggle.checked = false; updateToggleState(); });
optYes.addEventListener('click', () => { gcToggle.checked = true; updateToggleState(); });

// Initialize toggle
updateToggleState();


