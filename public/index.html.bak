			<script>
				// Toggle GC Selection
				function toggleGC(selection) {
					const btnFlow = document.getElementById('btn-flow');
					const btnEnvision = document.getElementById('btn-envision');
					const flowWorkflow = document.getElementById('flow-workflow');
					const envisionWorkflow = document.getElementById('envision-workflow');

					if (selection === 'flow') {
						btnFlow.classList.add('active');
						btnEnvision.classList.remove('active');
						flowWorkflow.classList.remove('hidden');
						envisionWorkflow.classList.add('hidden');
					} else {
						btnEnvision.classList.add('active');
						btnFlow.classList.remove('active');
						envisionWorkflow.classList.remove('hidden');
						flowWorkflow.classList.add('hidden');
					}
				}

				// Matrix Data
				const matrixData = [
					{ responsibility: "Site Selection & Due Diligence", phases: ["F", "S", "S", "-", "-"] },
					{ responsibility: "Entitlements & Zoning", phases: ["F", "F", "S", "-", "-"] },
					{ responsibility: "Architect Selection & Management", phases: ["F", "J", "J", "S", "-"] },
					{ responsibility: "Design Development", phases: ["S", "J", "J", "S", "-"] },
					{ responsibility: "Budget Development", phases: ["J", "J", "E", "E", "J"] },
					{ responsibility: "GC Procurement", phases: ["F", "J", "J", "-", "-"] },
					{ responsibility: "Subcontractor Management", phases: ["-", "S", "E", "E", "E"] },
					{ responsibility: "Construction Oversight", phases: ["-", "-", "J", "E", "E"] },
					{ responsibility: "Quality Control", phases: ["-", "-", "J", "E", "E"] },
					{ responsibility: "Schedule Management", phases: ["F", "J", "J", "E", "E"] },
					{ responsibility: "Owner Representation", phases: ["F", "F", "F", "F", "F"] },
					{ responsibility: "Financial Reporting", phases: ["F", "F", "J", "J", "F"] },
					{ responsibility: "Closeout & Turnover", phases: ["-", "-", "-", "J", "J"] }
				];

				const phaseNames = ["Pre-Dev", "Design", "Pre-Con", "Construction", "Close-Out"];

				// Render Desktop Matrix
				function renderDesktopMatrix() {
					const tbody = document.getElementById('matrix-body');
					tbody.innerHTML = matrixData.map(row => `
						<tr class="border-b border-gray-100 hover:bg-gray-50">
							<td class="p-3 font-medium">${row.responsibility}</td>
							${row.phases.map(phase => `
								<td class="p-3 text-center">
									${getCellBadge(phase)}
								</td>
							`).join('')}
						</tr>
					`).join('');
				}

				// Render Mobile Matrix
				function renderMobileMatrix(phaseIndex = 0) {
					const container = document.getElementById('mobile-matrix');
					const filtered = matrixData.filter(row => row.phases[phaseIndex] !== '-');

					container.innerHTML = filtered.map(row => `
						<div class="matrix-card bg-white border border-gray-200 rounded-lg p-4 mb-3">
							<div class="flex justify-between items-center">
								<span class="font-medium text-gray-900">${row.responsibility}</span>
								${getCellBadge(row.phases[phaseIndex])}
							</div>
						</div>
					`).join('') || '<p class="text-center text-gray-500 py-8">No responsibilities in this phase</p>';
				}

				// Get Badge HTML
				function getCellBadge(value) {
					const badges = {
						'F': '<span class="inline-flex items-center justify-center w-8 h-8 bg-black text-white text-xs font-bold rounded">F</span>',
						'E': '<span class="inline-flex items-center justify-center w-8 h-8 bg-indigo-600 text-white text-xs font-bold rounded">E</span>',
						'J': '<span class="inline-flex items-center justify-center w-8 h-8 bg-gradient-to-r from-black to-indigo-600 text-white text-xs font-bold rounded">J</span>',
						'S': '<span class="inline-flex items-center justify-center w-8 h-8 bg-gray-200 text-gray-600 text-xs font-bold rounded">S</span>',
						'-': '<span class="text-gray-300">—</span>'
					};
					return badges[value] || badges['-'];
				}

				// Phase Tab Handling
				document.querySelectorAll('.phase-tab').forEach(tab => {
					tab.addEventListener('click', function() {
						document.querySelectorAll('.phase-tab').forEach(t => t.classList.remove('active'));
						this.classList.add('active');
						renderMobileMatrix(parseInt(this.dataset.phase));
					});
				});

				// Deliverables Data
				const deliverables = [
					{
						phase: "Pre-Development",
						color: "bg-gray-800",
						items: ["Market Analysis", "Site Assessment", "Zoning Review", "Pro Forma Model", "LOI / PSA"]
					},
					{
						phase: "Design",
						color: "bg-indigo-600",
						items: ["Schematic Design", "Design Development", "MEP Coordination", "Permit Drawings", "Spec Book"]
					},
					{
						phase: "Pre-Construction",
						color: "bg-emerald-600",
						items: ["GMP Budget", "Subcontractor Bids", "Value Engineering", "Construction Schedule", "Permits"]
					},
					{
						phase: "Construction",
						color: "bg-amber-600",
						items: ["Weekly Reports", "Pay Applications", "RFI Log", "Change Orders", "Quality Reports"]
					},
					{
						phase: "Close-Out",
						color: "bg-purple-600",
						items: ["Punch List", "As-Builts", "O&M Manuals", "Warranties", "Final Accounting"]
					}
				];

				// Render Deliverables
				function renderDeliverables() {
					const grid = document.getElementById('deliverables-grid');
					grid.innerHTML = deliverables.map(phase => `
						<div class="gallery-card bg-white border border-gray-200 rounded-lg overflow-hidden">
							<div class="${phase.color} text-white p-4">
								<h3 class="font-bold text-lg">${phase.phase}</h3>
							</div>
							<ul class="p-4 space-y-2">
								${phase.items.map(item => `
									<li class="flex items-center text-sm text-gray-700">
										<svg class="w-4 h-4 mr-2 text-gray-400" fill="none" stroke="currentColor" viewBox="0 0 24 24">
											<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12l2 2 4-4m6 2a9 9 0 11-18 0 9 9 0 0118 0z"/>
										</svg>
										${item}
									</li>
								`).join('')}
							</ul>
						</div>
					`).join('');
				}

				// Initialize
				document.addEventListener('DOMContentLoaded', function() {
					renderDesktopMatrix();
					renderMobileMatrix(0);
					renderDeliverables();
				});
			</script>
		</body>
		</html>
